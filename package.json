{
    "name": "vault",
    "version": "1.0.0",
    "description": "Hackathon Vault",
    "author": "Long Trinh",
    "private": true,
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "start-worker:prod": "node dist/schedule.worker",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "cli": "ts-node -r tsconfig-paths/register src/cli.ts",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
        "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/database/db.migrate.config.ts",
        "typeorm:generate-migration": "npm run typeorm -- -d ./src/database/db.migrate.config.ts migration:generate ./src/database/migrations/$npm_config_name",
        "typeorm:create-migration": "npm run typeorm -- migration:create ./src/database/migrations/$npm_config_name",
        "typeorm:revert-migration": "npm run typeorm -- -d ./src/database/db.migrate.config.ts migration:revert"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.750.0",
        "@nestjs-modules/ioredis": "^2.0.2",
        "@nestjs/axios": "^4.0.0",
        "@nestjs/bull": "^11.0.0",
        "@nestjs/cache-manager": "^3.0.0",
        "@nestjs/common": "^11.0.1",
        "@nestjs/config": "^4.0.0",
        "@nestjs/core": "^10.4.1",
        "@nestjs/graphql": "^13.0.3",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/schedule": "^5.0.0",
        "@nestjs/swagger": "^11.0.2",
        "@nestjs/terminus": "^11.0.0",
        "@nestjs/typeorm": "^11.0.0",
        "@solana/web3.js": "1.98.0",
        "@typechain/ethers-v6": "^0.5.1",
        "axios": "^1.7.9",
        "bull": "^4.10.4",
        "cache-manager": "^6.3.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "crypto": "^1.0.1",
        "ethers": "^6.13.5",
        "graphql": "^16.10.0",
        "handlebars": "^4.7.8",
        "helmet": "^8.0.0",
        "hpagent": "^1.2.0",
        "lodash": "^4.17.21",
        "moment-timezone": "^0.5.41",
        "morgan": "^1.10.0",
        "nestjs-typeorm-paginate": "^4.0.4",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.13.1",
        "pino-http": "^10.4.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.2.0",
        "tweetnacl": "^1.0.3",
        "tweetnacl-util": "^0.15.1",
        "typeorm": "^0.3.20",
        "typeorm-naming-strategies": "^4.1.0",
        "uuidv4": "^6.2.13"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^10.1.3",
        "@nestjs/testing": "^9.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "29.2.4",
        "@types/lodash": "^4.14.191",
        "@types/morgan": "^1.9.4",
        "@types/node": "^22.13.5",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^8.18.2",
        "@typescript-eslint/parser": "^8.18.2",
        "better-sqlite3": "^9.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "29.3.1",
        "jest-environment-node-single-context": "^29.0.0",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "29.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.1.1",
        "typescript": "^5.1.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "moduleNameMapper": {
            "^@app/(.*)$": "<rootDir>/src/$1",
            "^@root(.*)$": "<rootDir>/src$1",
            "^@components(.*)$": "<rootDir>/src/components$1"
        },
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
